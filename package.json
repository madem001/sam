{
  "name": "edubattle-arena",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd src/backend && npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "backend:install": "cd src/backend && npm install",
    "backend:migrate": "cd src/backend && npm run prisma:push",
    "setup": "npm install && npm run backend:install && npm run backend:migrate"
  },
  "dependencies": {
    "@ionic/react": "^7.6.0",
    "@ionic/react-router": "^7.6.0",
    "ionicons": "^7.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "socket.io-client": "^4.8.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@ionic/cli": "^7.2.0",
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^8.2.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
